generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model product {
  id           Int     @id @unique(map: "id_UNIQUE") @default(autoincrement())
  product_name String  @db.VarChar(45)
  category     String  @db.VarChar(45)
  image_url    String? @db.VarChar(200)
}